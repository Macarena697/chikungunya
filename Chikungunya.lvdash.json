  "datasets": [
    {
      "name": "dataset1",
      "displayName": "complete_dataset_con_clima",
      "queryLines": [
        "SELECT * FROM workspace.default.complete_dataset_con_clima;"
      ]
    },
    {
      "name": "4339e580",
      "displayName": "predicciones_casos_confirmados_chikungunya",
      "queryLines": [
        "-- Parte 1: Datos Históricos Reales con Clima\r\n",
        "SELECT\r\n",
        "  date_trunc('month', fecha_notificacion) AS Mes,\r\n",
        "  COUNT(*) AS Casos,\r\n",
        "  NULL AS count_forecast,\r\n",
        "  -- CAMBIO: Agregar promedio de temperatura y suma de precipitación\r\n",
        "  AVG(temperatura_media) AS Temperatura_Media,\r\n",
        "  SUM(precipitacion) AS Precipitacion_Total_Mensual\r\n",
        "FROM\r\n",
        "  workspace.default.complete_dataset_con_clima\r\n",
        "WHERE\r\n",
        "  clasificacion_final = 'Chikungunya'\r\n",
        "GROUP BY\r\n",
        "  1 -- Agrupa por la primera columna (Mes)\r\n",
        "\r\n",
        "UNION ALL\r\n",
        "\r\n",
        "-- Parte 2: Datos Futuros Predichos\r\n",
        "SELECT\r\n",
        "  Mes,\r\n",
        "  NULL AS Casos,\r\n",
        "  casos_previstos AS count_forecast,\r\n",
        "  -- CAMBIO: Añadir columnas NULL para mantener la estructura de la tabla\r\n",
        "  NULL AS Temperatura_Media,\r\n",
        "  NULL AS Precipitacion_Total_Mensual\r\n",
        "FROM\r\n",
        "  workspace.default.predicciones_casos_confirmados_chikungunya\r\n",
        "\r\n",
        "ORDER BY\r\n",
        "  Mes ASC\r\n"
      ]
    },
    {
      "name": "71d4f997",
      "displayName": "complete_dataset_con_clima (Forecasted)",
      "queryLines": [
        "WITH original_table AS (\n",
        "    SELECT \n",
        "  DATE_TRUNC(\"MONTH\", `fecha_notificacion`) AS fecha_notificacion,\n",
        "  COUNT(*) AS count\n",
        "FROM (\n",
        "    SELECT * FROM workspace.default.complete_dataset_con_clima\n",
        ") GROUP BY 1\n",
        "  ),\n",
        "  dates AS (\n",
        "    SELECT \n",
        "      MAX(fecha_notificacion) AS max_date_column,\n",
        "      MIN(fecha_notificacion) AS min_date_column,\n",
        "      MAX_BY(count, fecha_notificacion) AS count\n",
        "    FROM original_table\n",
        "  ),\n",
        "  forecast_table AS (\n",
        "    SELECT \n",
        "      fecha_notificacion,\n",
        "      count_forecast,\n",
        "      count_upper,\n",
        "      count_lower,\n",
        "      NULL AS count\n",
        "    FROM AI_FORECAST(\n",
        "      TABLE(original_table),\n",
        "      horizon => (\n",
        "        SELECT\n",
        "          max_date_column + MAKE_DT_INTERVAL(CAST(FLOOR(DATEDIFF(max_date_column, min_date_column) * 0.5) AS INT), 0, 0, 0)\n",
        "        FROM dates\n",
        "      ),\n",
        "      time_col => 'fecha_notificacion',\n",
        "      value_col => 'count'\n",
        "    )\n",
        "  )\n",
        "\n",
        "  SELECT * FROM forecast_table\n",
        "  UNION ALL\n",
        "  SELECT\n",
        "    fecha_notificacion,\n",
        "    NULL AS count_forecast,\n",
        "    NULL AS count_upper,\n",
        "    NULL AS count_lower,\n",
        "    count\n",
        "  FROM original_table\n",
        "    WHERE fecha_notificacion < (SELECT max_date_column FROM dates)\n",
        "  UNION ALL\n",
        "  SELECT\n",
        "    max_date_column AS fecha_notificacion,\n",
        "    count AS count_forecast,\n",
        "    count AS count_upper,\n",
        "    count AS count_lower,\n",
        "    count\n",
        "  FROM\n",
        "    dates"
      ]
    }
  ],
